<h1>Listing personas</h1>

<table>
  <tr>
    <th>Tipopersona</th>
    <th>Nombre</th>
    <th>Apellidos</th>
    <th>Tipodocumento</th>
    <th>Numerodocumento</th>
    <th>Correo</th>
    <th>Clave</th>
    <th>Fechanacimiento</th>
    <th>Telefono</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @personas.each do |persona| %>
  <tr>
    <td><%= persona.tipopersona %></td>
    <td><%= persona.nombre %></td>
    <td><%= persona.apellidos %></td>
    <td><%= persona.tipodocumento %></td>
    <td><%= persona.numerodocumento %></td>
    <td><%= persona.correo %></td>
    <td><%= persona.clave %></td>
    <td><%= persona.fechanacimiento %></td>
    <td><%= persona.telefono %></td>
    <td><%= link_to 'Show', persona %></td>
    <td><%= link_to 'Edit', edit_persona_path(persona) %></td>
    <td><%= link_to 'Destroy', persona, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Persona', new_persona_path %>
